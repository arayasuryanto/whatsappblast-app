{
  "version": 2,
  "name": "whatsapp-blast-app",
  "builds": [
    {
      "src": "server-minimal.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server-minimal.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server-minimal.js": {
      "maxDuration": 60
    }
  }
}